{
    "manifest_version": 3,
    "name": "Goatify",
    "version": "0.1",
    "web_accessible_resources": [
        {
            "resources": ["img/*.jpg"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}